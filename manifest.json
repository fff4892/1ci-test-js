{
	"name": "ReplaceA",
	"version": "1.0",
	"description": "Replace all a's on random numbers",
	"permissions": ["storage"],
	"manifest_version": 3,
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"content_script": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_idle",
			"js": ["contentScript.js"]
		}
	]
}